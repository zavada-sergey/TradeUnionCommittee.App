@{
    ViewData["Title"] = "Dashboard";
}


<section class="add-user container-fluid">
    <div class="row justify-content-center">
        <div class="col-md-12 add-user_block">
            <h2 class="mt-3">Аналітика</h2>
            <ul class="nav nav-pills mt-3 mb-3" id="pills-tab" role="tablist">

                <li class="nav-item">
                    <a class="nav-link active" id="pills-profile-tab1" data-toggle="pill" href="#pills-profile1" role="tab" aria-controls="pills-profile" aria-selected="true">Tab #1</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab2" data-toggle="pill" href="#pills-profile2" role="tab" aria-controls="pills-profile" aria-selected="false">Tab #2</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab3" data-toggle="pill" href="#pills-profile3" role="tab" aria-controls="pills-profile" aria-selected="false">Tab #3</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab4" data-toggle="pill" href="#pills-profile4" role="tab" aria-controls="pills-profile" aria-selected="false">Tab #4</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab5" data-toggle="pill" href="#pills-profile5" role="tab" aria-controls="pills-profile" aria-selected="false">Tab #5</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab6" data-toggle="pill" href="#pills-profile6" role="tab" aria-controls="pills-profile" aria-selected="false">Tab #6</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab7" data-toggle="pill" href="#pills-profile7" role="tab" aria-controls="pills-profile" aria-selected="false">Tab #7</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab8" data-toggle="pill" href="#pills-profile8" role="tab" aria-controls="pills-profile" aria-selected="false">Tab #8</a>
                </li>
            </ul>
            <hr>
            <div class="tab-content" id="pills-tabContent">



                <!--Tab 1 start-->
                <div class="tab-pane fade show active" id="pills-profile1" role="tabpanel" aria-labelledby="pills-profile-tab1">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Pie Chart
                                </div>
                                <div class="card-body">
                                    <canvas id="myPieChart" width="100" height="48"></canvas>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 1 end-->
                <!--Tab 2 start-->
                <div class="tab-pane fade" id="pills-profile2" role="tabpanel" aria-labelledby="pills-profile-tab2">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Bar Chart
                                </div>
                                <div class="card-body">
                                    <canvas id="myBarChart" width="100%" height="50"></canvas>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 2 end-->
                <!--Tab 3 start-->
                <div class="tab-pane fade" id="pills-profile3" role="tabpanel" aria-labelledby="pills-profile-tab3">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Area Chart
                                </div>
                                <div class="card-body">
                                    <canvas id="myAreaChart" width="100%" height="30"></canvas>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 3 end-->
                <!--Tab 4 start-->
                <div class="tab-pane fade" id="pills-profile4" role="tabpanel" aria-labelledby="pills-profile-tab4">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Radar chart
                                </div>
                                <div class="card-body">
                                    <canvas id="radarChart"></canvas>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 4 end-->
                <!--Tab 5 start-->
                <div class="tab-pane fade" id="pills-profile5" role="tabpanel" aria-labelledby="pills-profile-tab5">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Line chart
                                </div>
                                <div class="card-body">
                                    <canvas id="lineChart"></canvas>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 5 end-->
                <!--Tab 6 start-->
                <div class="tab-pane fade" id="pills-profile6" role="tabpanel" aria-labelledby="pills-profile-tab6">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Bubble chart
                                </div>
                                <div class="card-body">
                                    <canvas id="bubbleChart"></canvas>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 6 end-->
                <!--Tab 7 start-->
                <div class="tab-pane fade" id="pills-profile7" role="tabpanel" aria-labelledby="pills-profile-tab7">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Task 1.1
                                </div>
                                <div class="card-body">
                                    <div id="heatmapChart"></div>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 7 end-->
                <!--Tab 8 start-->
                <div class="tab-pane fade" id="pills-profile8" role="tabpanel" aria-labelledby="pills-profile-tab8">
                    <div class="row">

                        <div class="col-lg-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    Task 1.3
                                </div>
                                <div class="card-body">
                                    <div id="basicColumnChart"></div>
                                </div>
                                <div class="card-footer small text-muted">Оновлено щойно - @DateTime.Now.ToString("dd.MM.yyyy hh:mm:ss")</div>
                            </div>
                        </div>

                    </div>
                </div>
                <!--Tab 8 end-->
            </div>
        </div>
    </div>
</section>

@section Scripts
{
    <environment include="Development">
        <script src="~/lib/chart.js/Chart.min.js"></script>
        <script src="~/lib/codepen/colour.palette.generator.js"></script>
        <script src="~/js/dashboard/chart-pie-demo.js"></script>
        <script src="~/js/dashboard/chart-bar-demo.js"></script>
        <script src="~/js/dashboard/chart-area-demo.js"></script>
        <script src="~/js/dashboard/chart-radar-demo.js"></script>
        <script src="~/js/dashboard/chart-line-demo.js"></script>
        <script src="~/js/dashboard/chart-bubble-demo.js"></script>



        <script src="~/js/dashboard/highcharts/highcharts.min.js"></script>
        <script src="~/js/dashboard/highcharts/exporting.min.js"></script>
        <script src="~/js/dashboard/highcharts/export-data.min.js"></script>
        <script src="~/js/dashboard/highcharts/accessibility.min.js"></script>
        <script src="~/js/dashboard/highcharts/heatmap.min.js"></script>

        <script src="~/js/dashboard/highcharts/heatmap/chart-heatmap-demo.js"></script>
        <script src="~/js/dashboard/highcharts/basicColumn/chart-basic-column-demo.js"></script>

    </environment>

    <environment exclude="Development">
        <script src="~/lib/chart.js/Chart.min.js" asp-append-version="true"></script>
        <script src="~/lib/codepen/colour.palette.generator.js" asp-append-version="true"></script>
        <script src="~/js/dashboard/chart-pie-demo.js" asp-append-version="true"></script>
        <script src="~/js/dashboard/chart-bar-demo.js" asp-append-version="true"></script>
        <script src="~/js/dashboard/chart-area-demo.js" asp-append-version="true"></script>
        <script src="~/js/dashboard/chart-radar-demo.js" asp-append-version="true"></script>
        <script src="~/js/dashboard/chart-line-demo.js" asp-append-version="true"></script>
        <script src="~/js/dashboard/chart-bubble-demo.js" asp-append-version="true"></script>
    </environment>
}